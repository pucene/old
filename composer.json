{
    "name": "pucene/pucene",
    "description": "Raw PHP implementation of apache lucene",
    "authors": [
        {
            "name": "Pucene Community",
            "homepage": "https://github.com/pucene/pucene/contributors"
        }
    ],
    "keywords": [
        "lucene",
        "indexing"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",

        "symfony/config": "^3.0",
        "symfony/console": "^3.0",
        "symfony/http-kernel": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/framework-bundle": "^3.0",

        "elasticsearch/elasticsearch": "^2.0",
        "ramsey/uuid": "^3.5",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/yaml": "^3.0",
        "jeroen/json-dump-reader": "^1.4",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pucene\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pucene\\Tests\\": "tests/src/"
        },
        "classmap": [
            "tests/app/AppKernel.php"
        ]
    }
}
