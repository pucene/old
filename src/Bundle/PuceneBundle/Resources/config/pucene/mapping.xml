<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="pucene.pucene.mapping.type_guesser" class="Pucene\Component\Pucene\Mapping\TypeGuesser"/>
        <service id="pucene.pucene.mapping" class="Pucene\Component\Pucene\Mapping\Mapping">
            <argument>%pucene.indices%</argument>
            <argument type="service" id="pucene.pucene.mapping.type_guesser"/>
            <argument type="service" id="pucene.pucene.analysis.standard"/>
        </service>
    </services>
</container>
